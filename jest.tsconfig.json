{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
        "dom",
        "dom.iterable",
        "esnext"
        ],
        "jsx": "react-jsx",
        "sourceMap": true,
        "experimentalDecorators": true,
        "noImplicitUseStrict": true,
        "removeComments": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "typeRoots": [
            "node_modules/@types"
        ]
    },
    "exclude": [
        "node_modules",
        "out",
        ".next"
    ]
}